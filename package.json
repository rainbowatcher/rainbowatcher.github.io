{
    "name": "rainbowatcher.github.io",
    "type": "module",
    "version": "0.0.4",
    "packageManager": "pnpm@10.3.0",
    "author": "rainbowatcher <rainbow-w@qq.com>",
    "license": "MIT",
    "repository": "https://github.com/rainbowatcher/rainbowatcher.github.io.git",
    "main": "index.js",
    "engines": {
        "node": ">=20",
        "pnpm": ">=9"
    },
    "scripts": {
        "analyze": "nuxi analyze",
        "clean": "nuxi cleanup && pnpm --stream -r clean",
        "check": "pnpm exec lychee -c lychee.toml -f markdown content",
        "build": "pnpm generate",
        "generate": "nuxi generate",
        "prepare": "nuxi prepare && simple-git-hooks",
        "preview": "nuxi preview",
        "docs:dev": "pnpm --filter docs docs:dev",
        "docs:build": "pnpm --filter docs docs:build",
        "dev": "nuxi dev",
        "lint": "eslint .",
        "lint:inspect": "config-inspector --open",
        "upgrade": "nuxi upgrade"
    },
    "devDependencies": {
        "@commitlint/config-conventional": "^19.7.1",
        "@docsearch/css": "^3.8.3",
        "@docsearch/js": "^3.8.3",
        "@eslint/config-inspector": "^1.0.0",
        "@iconify-json/line-md": "^1.2.5",
        "@iconify-json/mdi": "^1.2.3",
        "@iconify-json/ph": "^1.2.2",
        "@nuxt/content": "^3.1.1",
        "@nuxt/eslint": "^1.0.1",
        "@nuxt/fonts": "^0.10.3",
        "@nuxt/image": "^1.9.0",
        "@nuxtjs/algolia": "^1.10.2",
        "@nuxtjs/color-mode": "^3.5.2",
        "@nuxtjs/seo": "2.1.1",
        "@rainbowatcher/eslint-config": "^0.21.0",
        "@rainbowatcher/eslint-config-ignore": "^0.21.0",
        "@rainbowatcher/eslint-config-js": "^0.21.0",
        "@rainbowatcher/eslint-config-json": "^0.21.0",
        "@rainbowatcher/eslint-config-md": "^0.21.0",
        "@rainbowatcher/eslint-config-prettier": "^0.21.0",
        "@rainbowatcher/eslint-config-ts": "^0.21.0",
        "@rainbowatcher/eslint-config-unocss": "^0.21.0",
        "@rainbowatcher/eslint-config-vue": "^0.21.0",
        "@unocss/nuxt": "^65.4.3",
        "@vueuse/core": "^12.5.0",
        "@vueuse/motion": "^2.2.6",
        "@vueuse/nuxt": "^12.5.0",
        "better-sqlite3": "^11.8.1",
        "clsx": "^2.1.1",
        "commitlint": "^19.7.1",
        "cpx": "^1.5.0",
        "eslint": "^9.20.1",
        "katex": "^0.16.21",
        "mermaid": "^11.4.1",
        "nanoid": "^5.0.9",
        "nuxi": "^3.21.1",
        "nuxt": "^3.15.4",
        "radix-vue": "^1.9.14",
        "rehype-external-links": "^3.0.0",
        "rehype-katex": "^7.0.1",
        "rehype-parse": "^9.0.1",
        "rehype-stringify": "^10.0.1",
        "remark-flexible-containers": "^1.2.1",
        "remark-gfm": "^4.0.1",
        "remark-math": "^6.0.0",
        "rimraf": "^6.0.1",
        "serve": "^14.2.4",
        "shiki": "^2.3.2",
        "simple-git-hooks": "^2.11.1",
        "std-env": "^3.8.0",
        "tailwind-merge": "^3.0.1",
        "typescript": "^5.7.3",
        "vaul-vue": "^0.2.1"
